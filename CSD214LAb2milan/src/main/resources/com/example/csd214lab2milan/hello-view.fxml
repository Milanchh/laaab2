<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="355.0" prefWidth="590.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csd214lab2milan.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="uid" text="id"/>
   <TextField fx:id="uname" text="name"/>
   <TextField fx:id="uemail" text="email"/>
   <TextField fx:id="uadrress" text="adress"/>
   <Button mnemonicParsing="false" onAction="#insertdata" text="Insert" />
   <Button mnemonicParsing="false" onAction="#updatedata" text="Update" />
   <Button mnemonicParsing="false" onAction="#deletedata" text="Delete" />
   <Button mnemonicParsing="false" onAction="#loaddata" text="Load" />
   <TableView fx:id="tableview" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="ID" prefWidth="75.0" text="ID" />
       <TableColumn fx:id="NAME" prefWidth="75.0" text="NAME" />
         <TableColumn fx:id="EMAIL" prefWidth="75.0" text="EMAIL" />
         <TableColumn fx:id="ADRRESS" prefWidth="75.0" text="ADRRESS" />
     </columns>
   </TableView>

    <Label fx:id="welcomeText" />
   <Button mnemonicParsing="false" onAction="#fetchdata" text="fetch" />
</VBox>
